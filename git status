[1mdiff --git a/script.js b/script.js[m
[1mnew file mode 100644[m
[1mindex 0000000..1a679a2[m
[1m--- /dev/null[m
[1m+++ b/script.js[m
[36m@@ -0,0 +1,64 @@[m
[32m+[m[32m//starting off by creating a calender and then adding on too that[m
[32m+[m
[32m+[m
[32m+[m[32mconst prevButton = document.getElementById('prev');[m
[32m+[m[32mconst nextButton = document.getElementById('next');[m
[32m+[m
[32m+[m[32mconst daysContainer = document.getElementById('days');[m
[32m+[m[32mconst monthYear = document.getElementById("monthYear");[m
[32m+[m[32mlet currentDate = new Date();[m
[32m+[m
[32m+[m[32mfunction renderCalendar() {[m
[32m+[m[32m    const year = currentDate.getFullYear();[m
[32m+[m[32m    const month = currentDate.getMonth();[m
[32m+[m
[32m+[m[32m    const monthNames = [[m
[32m+[m[32m        'January', 'February', 'March', 'April', 'May', 'June', 'July',[m
[32m+[m[32m        'August', 'September', 'October', 'November', 'December'[m
[32m+[m[32m    ];[m
[32m+[m
[32m+[m[32m    monthYear.textContent = `${monthNames[month]} ${year}`;[m
[32m+[m[32m    daysContainer.innerHTML = '';[m
[32m+[m
[32m+[m[32m    const firstDay = new Date(year, month, 1).getDay();[m
[32m+[m[32m    const lastDate = new Date(year, month + 1, 0).getDate();[m
[32m+[m
[32m+[m[32m    // Add empty divs for alignment[m
[32m+[m[32m    for (let i = 0; i < firstDay; i++) {[m
[32m+[m[32m        const emptyDiv = document.createElement('div');[m
[32m+[m[32m        emptyDiv.classList.add('empty');[m
[32m+[m[32m        daysContainer.appendChild(emptyDiv);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Add actual day divs[m
[32m+[m[32m    for (let day = 1; day <= lastDate; day++) {[m
[32m+[m[32m        const dayDiv = document.createElement('div');[m
[32m+[m[32m        dayDiv.textContent = day;[m
[32m+[m[32m        dayDiv.classList.add('day');[m
[32m+[m
[32m+[m[32m        const today = new Date();[m
[32m+[m[32m        if ([m
[32m+[m[32m            day === today.getDate() &&[m
[32m+[m[32m            month === today.getMonth() &&[m
[32m+[m[32m            year === today.getFullYear()[m
[32m+[m[32m        ) {[m
[32m+[m[32m            dayDiv.classList.add('today');[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        daysContainer.appendChild(dayDiv);[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mprevButton.addEventListener('click', () => {[m
[32m+[m[32m    currentDate.setMonth(currentDate.getMonth() - 1);[m
[32m+[m[32m    renderCalendar();[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mnextButton.addEventListener('click', () => {[m
[32m+[m[32m    currentDate.setMonth(currentDate.getMonth() + 1);[m
[32m+[m[32m    renderCalendar();[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mrenderCalendar();[m
[32m+[m
[32m+[m
